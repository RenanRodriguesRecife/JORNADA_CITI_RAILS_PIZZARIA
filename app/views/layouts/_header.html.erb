<header>
    <div>
      <nav>
        <ul>
          <% if logged_in? %>
            <li><%= link_to "Home", companies_path %></li>
            <li>
              <ul>
                <li><%= link_to "Perfil", current_user %></li>
               <li><%= link_to "Editar Perfil",
edit_company_path(current_user) %></li>
                <li>
                  <%= link_to "Sair", sign_out_path, method: "delete" %>
                </li>
              </ul>
            </li>
          <% else %>
            <li><%= link_to "Log in", root_path %></li>
          <% end %>
        </ul>
      </nav>
    </div>
  </header>


